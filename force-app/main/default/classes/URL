String [] conFields= new String [] {'ID','DS_SFQB_Primary_Contact_on_Quickbook__c'};
                                    Map<String,Schema.SObjectField> conCheck = Schema.SObjectType.Contact.fields.getMap();
                                for (String fieldToCheck : conFields) {
                                    // Check if the user has create access on the each field
                                    if (conCheck.get(fieldToCheck)!= null && !conCheck.get(fieldToCheck).getDescribe().isAccessible()) {
                                        ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.FATAL,
                                                                                   'Insufficient access')); 
                                        
                                    }
                                }


